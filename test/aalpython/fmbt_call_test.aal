aal "noname" {
    language "python" {}

    # preview-hide-vars:
    # preview-depth: 5
    variables {
        default_heuristic
    }

    initial_state {
        default_heuristic = "lookahead(4)"
    }

    input "heuristic lookahead2random" {
        guard { return fmbt.getHeuristic()==default_heuristic }
        adapter { fmbt.setHeuristic("random") }
    }
    input "heuristic random2lookahead" {
        guard { return fmbt.getHeuristic()=="random" }
        adapter { fmbt.setHeuristic(default_heuristic) }
    }

